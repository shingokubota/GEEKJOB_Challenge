<!DOCTYPE html>
<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
        <title>JSP Page</title>
    </head>
    <%
    final int BASE=50;
    int num=30;
    int tasu=BASE+num;
    int hiku=BASE-num;
    int kakeru=BASE*num;
    int waru=BASE/num;
    int amaru=BASE%num;
    
    out.print(tasu);
    out.print(hiku);
    out.print(kakeru);
    out.print(waru);
    out.print(amaru);//それぞれを計算する
    
    out.print(++num);//+1して表示する
    out.print(num++);//31を表示した後、+1する
    out.print(num);//32を表示
    
    %>
</html>
